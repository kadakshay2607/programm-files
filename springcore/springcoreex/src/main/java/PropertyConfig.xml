<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
<bean id="prod" class="com.te.springcore.bean.Product">
<property name="pid" value="100" /> 
<property name="pname" value="mobile" />
<property name="map">
<map>
<entry key="plan" value="100"/>
<entry key="do" value="200"/>
<entry key="it" value="300"/>
</map>
</property>

<property name="list">
<list>
<value>john</value>
<value>hi</value>
<value>goa</value>
<value>smith</value>
</list>
</property> 
<property name="set">
<set>
<value>1</value>
<value>2</value>
<value>3</value>
<value>4</value>

</set>
</property>
 </bean>

</beans>