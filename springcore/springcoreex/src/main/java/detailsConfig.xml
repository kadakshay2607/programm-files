<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
<bean id="add" class="com.te.springcore.StudentAddress">
<property name="hno" value="100"/>
<property name="streetno" value="11"/>
<property name="state" value="mah"/>
<property name="country" value="india"/>
 </bean>
 <bean class="com.te.springcore.StudentDetails" id="deat">
 <property name="id" value="01"/>
 <property name="name" value="akshay"/>
 <property name="address" ref="add"/>
 
 </bean>

</beans>