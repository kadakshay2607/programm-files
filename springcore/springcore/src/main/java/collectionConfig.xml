<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"

	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- LIST BEAN -->
	<bean class="com.te.springcore.bean.Student" id="studentNames">

		<property name="names">
			<list>
				<value>John</value>
				<value>Sam</value>
				<value>Susan</value>
				<value>Robert</value>
				<value>Robert</value>
			</list>
		</property>
	</bean>

	<!-- SET BEAN -->
	<bean class="com.te.springcore.bean.Student" id="studentAddresses">
		<property name="addresses">
			<set>
				<value>London</value>
				<value>New york</value>
				<value>Belgium</value>
				<value>Italy</value>
				<value>Italy</value>
			</set>
		</property>

	</bean>

	<!-- MAP BEAN -->
	<bean class="com.te.springcore.bean.Student" id="studentCourses">
		<property name="courses">
			<map>
				<entry key="1" value="Java" />
				<entry key="2" value="Python" />
				<entry key="3" value="Hibernate" />
				<entry key="4" value="Spring" />
			</map>
		</property>

	</bean>
</beans>

	