<?xml version="1.0" encoding="UTF-8"?>
 <beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 
<context:annotation-config/>
 <bean class="com.te.springcoreannotation.bean.Engine" id="eng">
 <property name="cc" value="1000"/>
  <property name="fueltype" value="petol"/>
 </bean>
 <bean class="com.te.springcoreannotation.bean.Engine" id="engine" >
 <property name="cc" value="2000"/>
  <property name="fueltype" value="diesel"/>
 </bean>
 <bean class="com.te.springcoreannotation.bean.Car" id="car" >
 <property name="model" value="gtr"/>
  <property name="brand" value="audi"/>
 </bean>
 
</beans>